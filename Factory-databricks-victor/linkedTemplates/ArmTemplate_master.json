{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nome do Data Factory",
			"defaultValue": "Factory-databricks-victor"
		},
		"AzureDatabricksconexao_accessToken": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'accessToken' de 'AzureDatabricksconexao'"
		},
		"ls_azuresql_password": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'password' de 'ls_azuresql'"
		},
		"ls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Cadeia de caracteres segura para 'accountKey' de 'ls_datalake'"
		},
		"AzureDatabricksconexao_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1213-120747-xpod02ys"
		},
		"ls_azuresql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-cursoudemy-victor.database.windows.net"
		},
		"ls_azuresql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sql-cursoudemy-victor"
		},
		"ls_azuresql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "victor"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakeanacvictor.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI para a conta de armazenamento contendo os modelos do ARM vinculados"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token SAS para a conta de armazenamento contendo modelos do ARM vinculados"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Factory-databricks-victor_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricksconexao_accessToken": {
						"value": "[parameters('AzureDatabricksconexao_accessToken')]"
					},
					"ls_azuresql_password": {
						"value": "[parameters('ls_azuresql_password')]"
					},
					"ls_datalake_accountKey": {
						"value": "[parameters('ls_datalake_accountKey')]"
					},
					"AzureDatabricksconexao_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksconexao_properties_typeProperties_existingClusterId')]"
					},
					"ls_azuresql_properties_typeProperties_server": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_server')]"
					},
					"ls_azuresql_properties_typeProperties_database": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_database')]"
					},
					"ls_azuresql_properties_typeProperties_userName": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_userName')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}
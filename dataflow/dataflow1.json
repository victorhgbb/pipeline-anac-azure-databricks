{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_generico_csv",
						"type": "DatasetReference"
					},
					"name": "PlanCustomersData"
				},
				{
					"dataset": {
						"referenceName": "ds_generico_csv",
						"type": "DatasetReference"
					},
					"name": "PlanOrdersData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "JoinCustomerOrders"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PlanCustomersData",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PlanOrdersData",
				"PlanCustomersData, PlanOrdersData join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinCustomerOrders"
			]
		}
	}
}